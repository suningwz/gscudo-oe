<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="gs_message_wizard_view_form" model="ir.ui.view">
        <field name="name">gs_message_wizard_view_form</field>
        <field name="model">gs_message_wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <p>
                        <field name="message" nolabel="1" readonly="1" />
                    </p>
                </sheet>
                <footer>
                    <button
                        name="action_ok"
                        string="Ok"
                        type="object"
                        default_focus="1"
                        class="oe_highlight"
                    /> 
                </footer>
            </form>
        </field>
    </record>
</odoo>
